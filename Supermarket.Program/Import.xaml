<Window x:Class="Supermarket.Program.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        Title="Import" Height="300" Width="600">
    <Grid Margin="5,5,5,5">
        <ComboBox Margin="108,24,105.6,211.4">
            <ComboBox.ItemsPanel>
                <ItemsPanelTemplate>
                    <WrapPanel Orientation="Vertical" Height="100" />
                </ItemsPanelTemplate>
            </ComboBox.ItemsPanel>

            <ComboBoxItem Content="Value 1" />
            <ComboBoxItem Content="Value 2" />
            <ComboBoxItem Content="Value 3" />
            <ComboBoxItem Content="Value 4" Width="394" />
        </ComboBox>
        <TextBox x:Name="FileNameTextBox" HorizontalAlignment="Left" Margin="108,99,0,0" VerticalAlignment="Top" Height="30" Width="370" IsEnabled="False" />
        <TextBlock Name="ImportDates" Height="100" Margin="108,150,105.6,10.4" FontSize="14" FontWeight="Bold"/>
        <Button Content="Browse" HorizontalAlignment="Left" Margin="10,99,0,0" x:Name="Browse" VerticalAlignment="Top" Width="80" Height="30" Click="Browse_Click" />
        <Button Content="Back" HorizontalAlignment="Right" Margin="0,220,9.6,0" VerticalAlignment="Top" Width="80" Click="Back_Click" Height="30"/>



    </Grid>
</Window>
