<Window x:Class="Supermarket.Program.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Import" Height="300" Width="600">
    <Grid Margin="5,5,5,5">
        <TextBox x:Name="FileNameTextBox" HorizontalAlignment="Left" Margin="167,99,0,0" VerticalAlignment="Top" Height="30" Width="365" IsEnabled="False" />
        <Button Content="Browse" Height="30" HorizontalAlignment="Left" Margin="61,99,0,0" x:Name="Browse" VerticalAlignment="Top" Width="88" Click="Browse_Click" />
        <ComboBox Margin="167,22,51.6,213.4">
            <ComboBox.ItemsPanel>
                <ItemsPanelTemplate>
                    <WrapPanel Orientation="Vertical" Height="100" />
                </ItemsPanelTemplate>
            </ComboBox.ItemsPanel>

            <ComboBoxItem Content="Value 1" />
            <ComboBoxItem Content="Value 2" />
            <ComboBoxItem Content="Value 3" />
            <ComboBoxItem Content="Value 4" Width="394" />
        </ComboBox>
        <Button Content="Back" HorizontalAlignment="Left" Margin="494,220,0,0" VerticalAlignment="Top" Width="80" Click="Back_Click" Height="30"/>

    </Grid>
</Window>
