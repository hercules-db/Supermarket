<Window x:Class="Supermarket.Program.Export"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Export" Height="300" Width="600">
    <Grid Margin="5,5,5,5">
        <Button Content="Export" HorizontalAlignment="Left" Margin="47,82,0,0" VerticalAlignment="Top" Width="80" Height="30" Click="Export_Click"/>
        <ComboBox Margin="153,84,45.6,148.4">
            <ComboBox.ItemsPanel>
                <ItemsPanelTemplate>
                    <WrapPanel Orientation="Vertical" Height="100" />
                </ItemsPanelTemplate>
            </ComboBox.ItemsPanel>

            <ComboBoxItem Content="Value 1" />
            <ComboBoxItem Content="Value 2" />
            <ComboBoxItem Content="Value 3" />
            <ComboBoxItem Content="Value 4" Width="394" />
        </ComboBox>
        <Button Content="Back" HorizontalAlignment="Left" Margin="494,220,0,0" VerticalAlignment="Top" Width="80" Height="30" Click="Back_Click"/>

    </Grid>
</Window>
